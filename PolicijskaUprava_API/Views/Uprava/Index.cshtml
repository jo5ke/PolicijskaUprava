@{
    ViewBag.Title = "Uprava";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section scripts{
    <script type="text/javascript">
        $(function()
        {
            $.getJSON('/api/uprava', function (uprave)
                {
                    $(uprave).each(function(i, item)
                    {
                        $('#uprave').append('<li>' + item.Naziv + '</li>');
                    });
                });
        });
    </script>
}


<div id="body">
    <ul id="uprave"></ul>
</div>
